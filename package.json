{
  "name": "jamg-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.6.2",
    "boom": "^7.3.0",
    "bootstrap": "^4.1.3",
    "cryptiles": "^4.1.3",
    "firebase": "^5.7.0",
    "hoek": "^6.1.2",
    "jquery": "^3.3.1",
    "moment": "^2.23.0",
    "node-uuid": "^1.4.8",
    "popper.js": "^1.14.6",
    "query-string": "^6.2.0",
    "react": "^16.6.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3",
    "react-html-parser": "^2.0.2",
    "react-infinite-scroll-component": "^4.2.0",
    "react-linkify": "^0.2.2",
    "react-loader-spinner": "^2.3.0",
    "react-modal": "^3.7.1",
    "react-recaptcha": "^2.3.10",
    "react-redux": "^5.1.1",
    "react-redux-firebase": "^2.2.4",
    "react-router-dom": "^4.3.1",
    "react-toasts": "^2.0.14",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-firestore": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "no-map": "rimraf build/static/*/*.map",
    "start-js": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run no-map && firebase deploy",
    "deploy-all": "npm run build && npm run deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "2.1.1"
  }
}
